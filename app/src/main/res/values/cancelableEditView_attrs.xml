<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CancelableEditView">
        <attr name="editTextSize" format="dimension" />
        <attr name="hintColor" format="color" />
        <attr name="editColor" format="color" />
        <attr name="hintText" format="string" />
        <attr name="hintIcon" format="reference" />
        <attr name="cancelIcon" format="reference" />
        <attr name="maximumLength" format="integer" />
        <attr name="textType" format="enum">
            <enum name="number" value="1" />
            <enum name="password" value="2" />
        </attr>
        <attr name="singleLine" format="boolean" />
        <attr name="isWrapContent" format="boolean" />
    </declare-styleable>
</resources>